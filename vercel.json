{
  "headers": [
    {
      "source": "/assets/(.*).js",
      "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "VITE_API_BASE_URL": "https://visorestrategicobackendprepdd-aaejehbqgednb5fr.eastus2-01.azurewebsites.net",
    "VITE_APP_TITLE": "Visor de Proyectos Estrat√©gicos",
    "VITE_APP_VERSION": "1.0.0",
    "NODE_ENV": "production"
  }
}
